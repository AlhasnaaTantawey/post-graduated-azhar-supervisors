    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="azhar.supervisors.pg_azhar.ShowAllReportsInOneDepartment">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
